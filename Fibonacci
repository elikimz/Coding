
//Write a program generate the fibonacci sequence up to 100
public class fibonacci {
    public static void main(String[] args) {
        int length=100;
        //System.out.println(length);
       generateFibonacci(length);
    }
    public static void generateFibonacci (int length){
        int a=0,b=1,c;
        System.out.println(a);
        while((c=a+b)<=length){
            System.out.println(c+"");
            a=b;
            b=c;
        }
    }
    
}
